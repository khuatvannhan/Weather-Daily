<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item>
    <shape android:shape="oval">
        <stroke
            android:width="4dp"
            android:color="#FFF"/>
        <size android:width="80dp" android:height="80dp"/>
    </shape>
</item>

<item
    android:left="4dp"
    android:right="4dp"
    android:top="4dp"
    android:bottom="4dp">
    <shape android:shape="oval">
        <stroke
            android:width="1px"
            android:color="#9F9F9F"/>
    </shape>
</item>

    <item
        android:left="0px"
        android:right="0px"
        android:top="0px"
        android:bottom="0px">
        <shape android:shape="oval">
            <stroke
                android:width="1px"
                android:color="#9F9F9F"/>
        </shape>
    </item>
</layer-list>